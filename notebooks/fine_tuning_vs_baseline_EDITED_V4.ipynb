      "source": [
        "# Load the fine-tuned adapter for inference\n",
        "from peft import PeftModel\n",
        "import torch\n",
        "\n",
        "ft_model = PeftModel.from_pretrained(base_model, OUTPUT_DIR)\n",
        "ft_model.eval()\n",
        "\n",
        "print(\"Fine-tuned model dtype:\", next(ft_model.parameters()).dtype)\n",
        "\n",
        "# Turn off KV cache globally on this model\n",
        "ft_model.config.use_cache = False\n",
        "\n",
        "if hasattr(ft_model, \"generation_config\") and ft_model.generation_config is not None:\n",
        "    gen_conf = ft_model.generation_config\n",
        "    gen_conf.use_cache = False\n",
        "    # Make sure no cache implementation is active\n",
        "    if hasattr(gen_conf, \"cache_implementation\"):\n",
        "        gen_conf.cache_implementation = None\n",
        "    if hasattr(gen_conf, \"sliding_window\"):\n",
        "        gen_conf.sliding_window = None\n",
        "\n",
        "print(\"Loaded LoRA adapter for inference (cache disabled).\")\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "UClXXE5Evm-t"
      },
      "outputs": [],
      "source": [
        "from typing import Optional\n",
        "import torch\n",
        "\n",
        "def ask_finetuned(\n",
        "    question: str,\n",
        "    system_prompt: Optional[str] = SYSTEM_PROMPT,\n",
        "    max_new_tokens: int = 256,\n",
        ") -> str:\n",
        "    # Gemma template does NOT support a separate \"system\" role,\n",
        "    # so we inline the system prompt into the user content.\n",
        "    if system_prompt:\n",
        "        user_content = system_prompt.strip() + \"\\n\\n\" + question\n",
        "    else:\n",
        "        user_content = question\n",
        "\n",
        "    messages = [\n",
        "        {\"role\": \"user\", \"content\": user_content}\n",
        "    ]\n",
        "\n",
        "    # Build a single prompt string using Gemma's chat template\n",
        "    prompt = tokenizer.apply_chat_template(\n",
        "        messages,\n",
        "        tokenize=False,\n",
        "        add_generation_prompt=True,\n",
        "    )\n",
        "\n",
        "    inputs = tokenizer(prompt, return_tensors=\"pt\").to(ft_model.device)\n",
        "\n",
        "    # Use inference_mode (stricter than no_grad, allows these in-place ops)\n",
        "    with torch.inference_mode():\n",
        "        output_ids = ft_model.generate(\n",
        "            **inputs,\n",
        "            max_new_tokens=max_new_tokens,\n",
        "            do_sample=False,   # greedy, deterministic\n",
        "            # DO NOT pass temperature here; we keep defaults to avoid warnings\n",
        "            use_cache=False,   # extra safety, even though we disabled it in config\n",
        "        )\n",
        "\n",
        "    # Strip the prompt and decode only generated tokens\n",
        "    gen_ids = output_ids[0][inputs[\"input_ids\"].shape[1]:]\n",
        "    answer = tokenizer.decode(gen_ids, skip_special_tokens=True)\n",
        "    return answer.strip()\n",
        "\n",
        "\n",
        "# Test a single question\n",
        "ft = ask_finetuned(\"2. What specific bug fixes and security advisories were included in Python 3.12.3 (April 2024)?\")\n",
        "print(ft)\n"
      ]
    },
