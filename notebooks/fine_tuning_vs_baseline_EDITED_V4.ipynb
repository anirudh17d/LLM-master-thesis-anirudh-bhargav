        "DEMO_QUESTIONS = [\n",
        "    \"What was added in Python 3.12.2 released in March 2024?\",\n",
        "]\n",
        "\n",
        "for i, q in enumerate(DEMO_QUESTIONS, 1):\n",
        "    print(f\"\\nQ{i}: {q}\\n\")\n",
        "    ans = ask_baseline(q)\n",
        "    print(ans)\n",
        "\n"
      ]
