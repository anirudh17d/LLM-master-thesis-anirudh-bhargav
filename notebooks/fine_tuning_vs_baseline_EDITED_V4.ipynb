        "# Locate dataset (adjust this if running locally vs Colab)\n",
        "from pathlib import Path\n",
        "\n",
        "# Preferred local path\n",
        "DATA_PATHS = [\n",
        "    Path(\"data/processed/fine_tuning_train-v5.jsonl\"),\n",
